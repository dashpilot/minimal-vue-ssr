{
	"version": 2,
	"builds": [
		{
			"src": "vercel.js",
			"use": "@vercel/node"
		},
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/client"
			}
		}
	],
	"routes": [
		{
			"src": "/assets/(.*)",
			"dest": "/dist/client/assets/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/vercel.js"
		}
	]
}
